#!/usr/bin/env python3

import sys
import random

header = ['Frame', "Column 1"]
setup  = ['int', 'double']
fms    = ""
filename = None
row = None
col = None


def md(val):
	return f"{val:.4f}"

def gidx(valtype):
	if valtype == 'int':
		return '8'
	else:
		return '13'

def strfr(setup):
	global fms
	if len(setup) == 2: line = fms.format(setup[0], setup[1])
	elif len(setup) == 3: line = fms.format(setup[0], setup[1], setup[2])
	elif len(setup) == 4: line = fms.format(setup[0], setup[1], setup[2], setup[3])
