def gfmt(i, setup):
	global fms
	if len(setup) == 2: line = fms.format(i, md(random.random()))
	elif len(setup) == 3: line = fms.format(i, md(random.random()), md(random.random()))
	elif len(setup) == 4: line = fms.format(i, md(random.random()), md(random.random()), md(random.random()))
	elif len(setup) == 5: line = fms.format(i, md(random.random()), md(random.random()), md(random.random()), md(random.random()))
	elif len(setup) == 6: line = fms.format(i, md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()))
	elif len(setup) == 7: line = fms.format(i, md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()))
