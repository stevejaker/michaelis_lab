for i in range(len(sys.argv)):
	if sys.argv[i] in ['-f']:
		filename = sys.argv[i + 1]
	elif sys.argv[i] in ['-r', '--row']:
		row = int(sys.argv[i + 1])
	elif sys.argv[i] in ['-c', '--col']:
		col = int(sys.argv[i + 1])
		if col > 50:
			sys.exit("ERROR -- To Many Colomuns!")
if filename == None:
	filename = f'out_{col}x{row}.dat'

if filename == None or row == None or col == None:
	sys.exit("Insufficient Args")

while len(setup) < col:
	setup.append('double')

for index, val in enumerate(setup):
	fms = fms + "{:>" + f"{gidx(val)}" +"} "
	header.append(f"Column {index + 1}")

fms += '\n'

with open(filename,'w') as f:
	f.write(strfr(header))
	for i in range(row):
		f.write(gfmt(i + 1, setup))

			