	elif len(setup) == 9: line = fms.format(i, md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()))
	elif len(setup) == 10: line = fms.format(i, md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()))
	elif len(setup) == 11: line = fms.format(i, md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()), md(random.random()))
